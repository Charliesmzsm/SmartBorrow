<view class="container"><view class="container_contact"><view class="cu-bar justify-start "><view class="action sub-title"><text class="text-xl text-bold text-green">关于我们</text><text class="bg-green" style="width:4rem;"></text></view></view><block wx:for="{{contact}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="container_contact_word"><image style="width:36rpx;height:36rpx;" src="{{item.img}}" mode="widthFix"></image><view style="width:130rpx;margin-left:10rpx;">{{item.title+":"}}</view><view>{{item.word}}</view></view></block><view class="container_team" style="margin-top:50rpx;"><view class="cu-bar justify-start "><view class="action sub-title"><text class="text-xl text-bold text-green">开发团队</text><text class="bg-green" style="width:4rem;"></text></view></view><scroll-view class="container_team_container" scroll-x="true"><block wx:for="{{team}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="container_team_item team"><view class="team_item_box"><view class="box_name"><text>{{item.name}}</text><text>{{item.grade+"级"}}</text></view><view class="box_other"><image style="width:40rpx;height:40rpx;margin-right:10rpx;" src="../../static/contact/gangwei.png" mode="widthFix"></image><view>{{item.position}}</view></view><view class="box_other"><image style="width:40rpx;height:40rpx;margin-right:10rpx;margin-top:10rpx;" src="../../static/contact/icon-.png" mode="widthFix"></image><view>{{item.start_time+"-"+item.end_time}}</view></view></view></view></block></scroll-view></view><view class="container_version"><view class="cu-bar justify-start "><view class="action sub-title"><text class="text-xl text-bold text-green">开发版本</text><text class="bg-green" style="width:4rem;"></text></view></view><scroll-view class="container_team_container" scroll-x="true"><block wx:for="{{version}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="container_team_item version"><view class="team_item_box"><view class="box_number"><image style="width:30rpx;height:30rpx;margin-right:10rpx;" src="../../static/contact/版本号.png" mode="widthFix"></image><text>{{item.version+"版本"}}</text></view><view id="box_content">{{item.change}}</view></view></view></block></scroll-view></view></view></view>